{
  "name": "scriptura",
  "displayName": "scriptura",
  "publisher": "pkaylabs",
  "description": "Scriptura is a VSCode package that displays a scripture every minutes.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "scriptura.scriptura",
        "title": "Scriptura"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.96.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.16.0"
  },
  "dependencies": {
    "xlsx": "^0.18.5"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/DevPrinceK/scriptura.git"
  }
}
